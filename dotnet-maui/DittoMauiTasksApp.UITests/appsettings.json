{
  "TestConfiguration": {
    "DefaultTimeoutSeconds": 60,
    "SyncTimeoutSeconds": 120,
    "ElementWaitTimeoutSeconds": 30,
    "ScreenshotOnFailure": true,
    "ScreenshotOnSuccess": false,
    "LogLevel": "Information"
  },
  "BrowserStack": {
    "ProjectName": "Ditto MAUI Tasks",
    "BuildName": "Integration Tests",
    "Video": true,
    "DeviceLogs": true,
    "NetworkLogs": true,
    "Debug": true,
    "AutoGrantPermissions": true
  },
  "Ditto": {
    "SyncRetryAttempts": 3,
    "SyncRetryDelayMs": 5000,
    "CollectionName": "tasks",
    "DocumentTimeoutSeconds": 60
  },
  "GitHub": {
    "DocumentIdPrefix": "github_test_",
    "TitlePrefix": "GitHub Test Task",
    "ExpectedFields": ["_id", "title", "done", "deleted"]
  }
}