  <script>
    var headerProductsSignedIn = (function() {
  return {
    init: init
  };
  function init() {
    var headerHabitat = document.getElementsByClassName('header-habitat');
    if (headerHabitat[0] && headerHabitat[0].innerHTML.length > 0) {
      uiBindings();
    }
  }

  function uiBindings() {
    var entered = [],
      enteredDropdown = false;
    const tabs = document.querySelectorAll('.products__menu-item');
    tabs.forEach(function(singleElement) {
      singleElement.addEventListener('mouseenter', function() {
        if (singleElement.id === 'products__product-panel-item--2') {
          document.querySelectorAll('.products__product-panel-padding').forEach(function(item) {
            item.style.animationDuration = '0.2s';
          });
        }
      });
    });
    const setActiveElement = function() {
      document.querySelectorAll('.products__menu-item.active').forEach(function(el) {
        el.classList.remove('active');
      });
      document.querySelectorAll('.products__product-panel-item.hide').forEach(function(el) {
        el.classList.remove('hide');
      });
      var target = document.querySelectorAll('#products__product-panel-item--1'),
        productContainerTarget = document.querySelectorAll('.products__product-panel-item--2');
      target.forEach(function(item) {
        item.classList.add('active');
      });
      productContainerTarget.forEach(function(item) {
        item.classList.add('hide');
      });
    };
    document.querySelectorAll('.bstack-product-menu-button').forEach(function(hoverButton) {
      hoverButton.addEventListener('mouseenter', setActiveElement);
      hoverButton.addEventListener('ontouchstart', setActiveElement);
    });

    document.querySelectorAll('.product-list-dropdown--experiment').forEach(function(hoverProductLink) {
      hoverProductLink.addEventListener('mouseenter', function() {
        if (!enteredDropdown) {
          enteredDropdown = true;
        }
      });
    });

    var dropdownToggle = document.querySelectorAll('.bstack-product-list-dropdown');

    if (dropdownToggle) {
      for (var i = 0; i < dropdownToggle.length; i++) {
        var productToggle = dropdownToggle[i].firstElementChild,
          productDropdown = dropdownToggle[i].querySelector('.' + productToggle.getAttribute('data-target'));

        dropdownToggle[i].addEventListener('mouseover', function() {
          if (this.firstElementChild.classList.contains('focussed')) {
            return false;
          }
          if (!this.firstElementChild.classList.contains('is-hover') && !enteredDropdown) {
            enteredDropdown = true;
          }
          this.firstElementChild.classList.add('is-hover');
          var activeDropdown = this.firstElementChild.getAttribute('data-target'),
            element = this.querySelector('.' + activeDropdown);
          openDropdown(element);
        });

        dropdownToggle[i].addEventListener('mouseleave', function() {
          document.querySelectorAll('.products__product-panel-padding').forEach(function(item) {
            item.style.animationDuration = '0s';
          });
          if (this.firstElementChild.classList.contains('focussed')) {
            return false;
          }
          this.firstElementChild.classList.remove('is-hover');
          var activeDropdown = this.firstElementChild.getAttribute('data-target'),
            element = this.querySelector('.' + activeDropdown);
          element.classList.remove('active');
          this.classList.remove('active');
        });

        dropdownToggle[i].addEventListener('focusout', function(event) {
          var activeDropdown = this.firstElementChild.getAttribute('data-target'),
            element = this.querySelector('.' + activeDropdown);
          if (!this.contains(event.relatedTarget)) {
            closeDropdown(element, this.firstElementChild);
          }
        });

        dropdownToggle[i].addEventListener('keydown', function(event) {
          var activeDropdown = this.firstElementChild.getAttribute('data-target'),
            element = this.querySelector('.' + activeDropdown);
          if (event.key === 'Escape' || event.keyCode === 27) {
            closeDropdown(element, this.firstElementChild);
            this.firstElementChild.focus();
          }
        });

        productToggle.addEventListener('click', function() {
          if (this.classList.contains('is-hover')) {
            return false;
          }
          var activeDropdown = this.getAttribute('data-target'),
            element = this.parentNode.querySelector('.' + activeDropdown),
            isAlreadyClosed = closeDropdown(element, this);
          if (isAlreadyClosed) {
            return false;
          }
          openDropdown(element);
          this.classList.add('focussed');
          this.setAttribute('aria-expanded', 'true');
        });

        var tablists = productDropdown.querySelector('[role=tablist]');
        tabsAutomatic(tablists, productDropdown);

        tabsOnHover(tablists, productDropdown);
      }
    }
  }

  function tabsOnHover(groupNode, dropdownContainerNode) {
    var productTabHover = Array.from(groupNode.querySelectorAll('[role=tab]')),
      tabpanel,
      tab;
    // eventFlag = [];
    for (var l = 0; l < productTabHover.length; l++) {
      tab = productTabHover[l];
      tab.addEventListener('mouseover', function() {
        var that = this;
        tabpanel = dropdownContainerNode.querySelector('.' + that.getAttribute('aria-controls'));
        for (var m = 0; m < productTabHover.length; m++) {
          tabpanel = dropdownContainerNode.querySelector('.' + productTabHover[m].getAttribute('aria-controls'));
          if (that == productTabHover[m]) {
            productTabHover[m].classList.add('active');
            tabpanel.classList.remove('hide');
          } else {
            productTabHover[m].classList.remove('active');
            tabpanel.classList.add('hide');
          }
        }
      });
    }
  }

  function tabsAutomatic(groupNode, dropdownContainerNode) {
    var tablistNode = groupNode,
      firstTab = null,
      lastTab = null,
      tabs = Array.from(tablistNode.querySelectorAll('[role=tab]')),
      tabpanels = [];

    for (var i = 0; i < tabs.length; i += 1) {
      var tab = tabs[i],
        tabpanel = dropdownContainerNode.querySelector('.' + tab.getAttribute('aria-controls'));

      tab.tabIndex = -1;
      tab.setAttribute('aria-selected', 'false');
      tabpanels.push(tabpanel);

      tab.addEventListener('keydown', tabOnKeydown.bind(this));
      tab.addEventListener('click', tabOnClick.bind(this));

      if (!firstTab) {
        firstTab = tab;
      }
      lastTab = tab;
    }

    setSelectedTab(firstTab, false);

    function setSelectedTab(currentTab, setFocus) {
      if (typeof setFocus !== 'boolean') {
        setFocus = true;
      }
      for (var n = 0; n < tabs.length; n += 1) {
        var tabTemp = tabs[n];
        if (currentTab === tabTemp) {
          tabTemp.setAttribute('aria-selected', 'true');
          tabTemp.removeAttribute('tabindex');
          tabTemp.classList.add('active');
          tabpanels[n].classList.remove('hide');
          if (setFocus) {
            tabTemp.focus();
          }
        } else {
          tabTemp.setAttribute('aria-selected', 'false');
          tabTemp.tabIndex = -1;
          tabTemp.classList.remove('active');
          tabpanels[n].classList.add('hide');
        }
      }
    }

    function setSelectedToPreviousTab(currentTab) {
      var index;

      if (currentTab === firstTab) {
        setSelectedTab(lastTab);
      } else {
        index = tabs.indexOf(currentTab);
        setSelectedTab(tabs[index - 1]);
      }
    }

    function setSelectedToNextTab(currentTab) {
      var index;

      if (currentTab === lastTab) {
        setSelectedTab(firstTab);
      } else {
        index = tabs.indexOf(currentTab);
        setSelectedTab(tabs[index + 1]);
      }
    }
    function tabOnKeydown(event) {
      var tgt = event.currentTarget,
        flag = false;

      switch (event.key) {
        case 'ArrowUp':
          setSelectedToPreviousTab(tgt);
          flag = true;
          break;

        case 'ArrowDown':
          setSelectedToNextTab(tgt);
          flag = true;
          break;

        case 'Home':
          setSelectedTab(firstTab);
          flag = true;
          break;

        case 'End':
          setSelectedTab(lastTab);
          flag = true;
          break;

        default:
          break;
      }

      if (flag) {
        event.stopPropagation();
        event.preventDefault();
      }
    }

    function tabOnClick(event) {
      setSelectedTab(event.currentTarget);
    }
  }

  function openDropdown(dropdownElement) {
    dropdownElement.dataset.isopen = 'true';
    dropdownElement.classList.add('active');
    dropdownElement.parentElement.classList.add('active');
  }

  function closeDropdown(dropdownElement, buttonElement) {
    var isDropdownOpenByClick = buttonElement.classList.contains('focussed'),
      isDropdownOpenByHover = dropdownElement.dataset.isopen;
    if (isDropdownOpenByClick && isDropdownOpenByHover === 'true') {
      dropdownElement.dataset.isopen = 'false';
      dropdownElement.classList.remove('active');
      dropdownElement.parentElement.classList.remove('active');
      buttonElement.classList.remove('focussed');
      buttonElement.setAttribute('aria-expanded', 'false');
      return true;
    }
    return false;
  }
})();

