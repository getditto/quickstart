      $('#primary-menu-toggle').focus();
    }
  });

  $('.header-habitat__pricing-link, .header-habitat__pricing-primary-cta').click(function() {
    WebEventTracker.logEvent([], EDS.webEvents, 'ClickHeaderPlansAndPricing', {
      referrer: window.location.href,
      abExp: window.BrowserStackConfig.amplitudeAbExperiments
    });
  });

  $('#invite-team-menu').click(function() {
    if (
      typeof userOnLiveDashboard === 'function' &&
      typeof userOnAppLiveDashboard === 'function' &&
      userOnLiveDashboard() &&
      !userOnAppLiveDashboard()
    ) {
      WebEventTracker.logEvent(['amplitude'], EDS.liveWebEvents, 'InteractWithAccountMenu', {
        action: 'InviteTeam'
      });
    }
  });

  $('#header-unlock-trail-cta').click(function() {
    let source = 'Live Dashboard';
    if (userOnAppLiveDashboard()) {
      source = 'App Live Dashboard';
    }

    if (BrowserStack.user.paywallPhase4ModalData && BrowserStack.user.paywallPhase4ModalData.showHeaderCTA) {
      WebEventTracker.logEvent([], EDS.webEvents, 'Paywall_PU_Click_Unlock_Pro_CTA', {
        user_role: BrowserStack.user.userRole,
        source: source,
        feature: 'None',
        team: 'online_sales'
      });
      window.pubSub.publish('loadPayWallFeatureTrialModal', {
        source: source
      });
    }
  });

  $('.promo-banner-buy-now').click(function() {
    WebEventTracker.logEvent([], EDS.webEvents, 'InteractedwithFreeProdBanner', {
      source: dashboard,
      action: 'Buy Now',
      placement: 'Hello bar',
      team: 'online_sales',
      user_role: BrowserStack.user.userRole,
      message: paywallFreeOfferMessage
    });
  });

  $('a.product-name-link').click(function() {
    WebEventTracker.logEvent([], EDS.webEvents, 'InteractedwithFreeProdBanner', {
      source: dashboard,
      action: $(this).text(),
      placement: 'Hello bar',
      team: 'online_sales',
      user_role: BrowserStack.user.userRole,
      message: paywallFreeOfferMessage
    });
  });

  $('.promo-banner-contact-owner').click(function(e) {
    e.preventDefault();
    const paywallRemovalFreeOfferData = BrowserStack.user.paywallRemovalFreeOfferData,
      productsData = paywallRemovalFreeOfferData.eft_products_data,
      duration = paywallRemovalFreeOfferData.duration,
      isOnLiveOrAppLiveDashboard = dashboard.indexOf('Live') > -1;
    var data = {
      product_name: productsData[0].name_to_display,
      duration: duration,
      on_live_or_app_live_dashboard: isOnLiveOrAppLiveDashboard
    };
    WebEventTracker.logEvent([], EDS.webEvents, 'InteractedwithFreeProdBanner', {
      source: dashboard,
      action: 'Contact Owner',
      placement: 'Hello bar',
      team: 'online_sales',
      user_role: BrowserStack.user.userRole,
      message: paywallFreeOfferMessage
    });
    jQuery.ajax({
      url: Config.generate_url('/user/paywall-contact-owner'),
      type: 'POST',
      data: data,
      header: {
        Accept: 'application/json'
      },
      success: function(contactOwnerSuccessResponse) {
        if (typeof Reactor !== 'undefined') {
          const notificationType = contactOwnerSuccessResponse.already_notified ? 'info' : 'success';
          Reactor.showCustomSnackbar(notificationType, contactOwnerSuccessResponse.message);
        }
      }
    });
  });

  $('.promo-banner-contact-us').click(function(e) {
    e.preventDefault();
    WebEventTracker.logEvent([], EDS.webEvents, 'InteractedwithFreeProdBanner', {
      source: dashboard,
      action: 'Contact Us',
      placement: 'Hello bar',
      team: 'online_sales',
      user_role: BrowserStack.user.userRole,
      message: paywallFreeOfferMessage
    });
    let message = 'Hi, I want to know more about the ',
      productsData = BrowserStack.user.paywallRemovalFreeOfferData.eft_products_data;
    productsData.forEach(function(product, index) {
      message += product.name_to_display;
      if (productsData.length - 1 != index) {
        message += ', ';
      }
    });
    message += ' and the complimentary access offered to my team. Can you please help me?';
    const name = window.userDetails.name,
      email = window.userDetails.email,
      company = window.userDetails.company,
      isOnLiveOrAppLiveDashboard = dashboard.indexOf('Live') > -1;

    var data = {
      name: name,
      email: email,
      company: company,
      message: message,
      query_type: 1,
      web_utm_source: 'free_product_access',
      request_type: 'Sales Enquiry',
      is_on_live_or_app_live_dashboard: isOnLiveOrAppLiveDashboard,
      lead_source: Constants.LEAD_SOURCE_MAP.csf
    };
    jQuery.ajax({
      url: Config.generate_url('/contact'),
      type: 'POST',
      data: data,
      header: {
        Accept: 'application/json'
      },
      success: function() {
        if (typeof Reactor !== 'undefined') {
          Reactor.showCustomSnackbar(
            'success',
            'Thank you for your interest!',
            'Our customer support team will be contacting you shortly.',
            'paywall-snackbar-wrapper'
          );
        }
      }
    });
  });

  $('.ftu-header-cta').click(function() {
    if (BrowserStack.user.ftu_nudge_data) {
      let ftuEventName = BrowserStack.user.ftu_nudge_data.free_user
        ? 'FT_Addition_Request_Profile'
        : 'Owner_Addition_Request_Profile';
      if (BrowserStack.user.ftu_nudge_data.free_user) {
        window.pubSub.publish('group-join-request-modal', { source: dashboard, trigger: 'profile' });
      }
      WebEventTracker.logEvent([], EDS.webEvents, ftuEventName, {
        source: dashboard,
        action: 'click_cta',
        team: 'online_sales'
      });
    }
  });

  jQuery(document).on('scroll', function() {
    var $this = $(this),
      stickyHeaderHeight = jQuery('.header-habitat.sticky-primary-header').height() || 0;
    if (stickyHeaderHeight && $this.scrollTop() > stickyHeaderHeight && $this.width() >= 980) {
      $('.free-trial-link > a')
        .removeClass('btn-secondary-white')
        .addClass('btn-primary');
    } else {
      $('.free-trial-link > a')
        .removeClass('btn-primary')
        .addClass('btn-secondary-white');
    }
  });

  function toolTipInit() {
    var toolTip = $('.uu-invite-tooltip'),
      toolTipWrapper = $('.uu-invite-tooltip-wrapper');
    if (toolTip.length < 1) {
      return;
    }
    toolTipWrapper.addClass('selected');
    WebEventTracker.logEvent([], EDS.webEvents, 'AutomateUUReminderModalViewed_AutomateDashboard', {});
    $('#uu-invite-tooltip-cancel-button').click(function() {
      var data = {
        keys_to_add: {
          dismissed_uu_invite_user_tooltip: true
        }
      };
      jQuery.ajax({
        url: Config.generate_url('/user/add-user-properties'),
        type: 'POST',
        data: data,
        header: {
          Accept: 'application/json'
        }
      });
      toolTip.addClass('hide');
      toolTipWrapper.removeClass('selected');
      WebEventTracker.logEvent([], EDS.webEvents, 'AutomateUUReminderModalClosed_AutomateDashboard', {});
    });

    $('#invite-link').click(function() {
      var data = {
        keys_to_add: {
          dismissed_uu_invite_user_tooltip: true
        }
      };
      jQuery.ajax({
        url: Config.generate_url('/user/add-user-properties'),
        type: 'POST',
        data: data,
        header: {
          Accept: 'application/json'
        }
      });
      toolTip.addClass('hide');
      toolTipWrapper.removeClass('selected');
      WebEventTracker.logEvent([], EDS.webEvents, 'InviteYourTeamClicked_Header_AutomateDashboard', {});
    });

    $('.feedback-notification__button, .dropdown__head-text, .dropdown__head').on('click', function() {
      toolTip.addClass('hide');
      toolTipWrapper.removeClass('selected');
    });

    $('.product-menu-toggle, #help-menu-toggle').on('mouseover', function() {
      toolTip.addClass('hide');
      toolTipWrapper.removeClass('selected');
    });
  }

  function getUtmParamsFromUrl() {
    var urlSearchParams = new URLSearchParams(window.location.search),
      params = Object.fromEntries(urlSearchParams.entries());
    return Object.fromEntries(
      Object.entries(params).filter(function(key) {
        return key[0].startsWith('utm_');
      })
    );
  }

  function setCookie(name, params) {
    if (params && !$.isEmptyObject(params)) {
      var paramsString = JSON.stringify(params);
      Cookie.create(name, paramsString, 0.0209);
    }
  }

  if (windowWidth > 1300 && windowWidth < 1360) {
    $('.cross-product-nav')
      .removeClass('col-lg-9')
      .addClass('col-lg-10');

    $('.js-product-list').addClass('products-tab');
  }
  var params = getUtmParamsFromUrl(),
    anchorElements = document.getElementsByTagName('a');
  setCookie('user_utm_data', params);
  for (let idx = 0; idx < anchorElements.length; ++idx) {
    var urlString = anchorElements[idx].href;
    if (urlString !== null && urlString !== '' && !anchorElements[idx].classList.contains('no-override')) {
      var splitURL = urlString.split('?'),
        queryString = new URLSearchParams(splitURL[1]),
        newParamString = '',
        uniqueDict = {};

      for (var param in params) {
        if (Object.prototype.hasOwnProperty.call(params, param)) {
          uniqueDict[param] = params[param];
        }
      }
      queryString.forEach(function(value, key) {
        uniqueDict[key] = value;
      });

      Object.entries(uniqueDict).forEach(function(uniquePair) {
        newParamString = newParamString + uniquePair[0] + '=' + uniquePair[1] + '&';
      });
      if (newParamString.charAt(newParamString.length - 1) === '&') {
        newParamString = newParamString.substr(0, newParamString.length - 1);
      }
      if (newParamString.charAt(newParamString.length - 1) === '?') {
        newParamString = newParamString.substr(0, newParamString.length - 1);
      }
      if (newParamString !== '') {
        anchorElements[idx].href = splitURL[0] + '?' + newParamString;
      }
    }
  }
  $('#bs-header-logo').on('click', function() {
    if (window.location.pathname == '/user/group-request') {
      window.WebEventTracker.logEvent([], EDS.webEvents, 'FT_Addition_Request_Screen', { action: 'skip_logo' });
    }
  });
});
    </script>

